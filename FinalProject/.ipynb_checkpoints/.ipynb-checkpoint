    "map_mi = folium.Map(location=[latitude, longitude], zoom_start=10)\n",
    "\n",
    "# add markers to map\n",
    "for lat, lng, TouristPlace in zip(TouristPlaceList['Latitude'], TouristPlaceList['Longitude'], TouristPlaceList['TouristPlace']):\n",
